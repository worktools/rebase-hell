    "latest-version": "^6.0.0",
    "md5": "^2.3.0",
    "nanoid": "^3.3.3",
    "randomcolor": "^0.6.2",
    "url-parse": "^1.5.10",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "bottom-tip": "^0.1.3",
    "copy-text-to-clipboard": "^3.0.1",
